<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <servlet>
        <servlet-name>check1</servlet-name>
        <servlet-class>Servlets.check1</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>add</servlet-name>
        <servlet-class>Servlets.add</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Demand_car</servlet-name>
        <servlet-class>Servlets.Demand_car</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Delete_car</servlet-name>
        <servlet-class>Servlets.Delete_car</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>vehicle</servlet-name>
        <servlet-class>Servlets.vehicle</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>check1</servlet-name>
        <url-pattern>/check1</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>add</servlet-name>
        <url-pattern>/add</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Demand_car</servlet-name>
        <url-pattern>/Demand_car</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Delete_car</servlet-name>
        <url-pattern>/Delete_car</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>vehicle</servlet-name>
        <url-pattern>/vehicle</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
